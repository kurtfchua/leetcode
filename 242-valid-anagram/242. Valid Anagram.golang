func isAnagram(s string, t string) bool {
    if len(s) != len(t){
        return false
    }

    counts := make([]int, 26)

    for i := 0; i < len(s); i ++{
        counts[s[i]-'a'] ++
        counts[t[i]-'a'] --

    }

    for _, num := range counts{
        if num != 0{
            return false
        }
    }
    
    return true
}