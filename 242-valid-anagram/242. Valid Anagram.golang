func isAnagram(s string, t string) bool {
    if len(s) != len(t){
        return false
    }

    hashmap := make(map[byte]int)

    for i := 0; i < len(s); i ++{
        hashmap[s[i]] ++
        hashmap[t[i]] --

    }

    for _, num := range hashmap{
        if num != 0{
            return false
        }
    }
    
    return true
}